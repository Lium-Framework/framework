{
    "name": "lium/framework",
    "type": "library",
    "description": "Micro framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Hermann",
            "email": "florian.hermann94@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-httphandlerrunner": "^1.1",
        "nyholm/psr7-server": "^0.4",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/routing": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "nyholm/psr7": "^1.1",
        "symfony/var-dumper": "^5.0",
        "vimeo/psalm": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Lium\\Framework\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
